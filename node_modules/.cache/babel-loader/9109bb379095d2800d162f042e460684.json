,CAAA,CACA,IADA,CACA,UAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,KAAA,EAFA,CAGA;AACA,OALA,EAMA,KANA,CAMA,UAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,IAAA,EAAA,CAAA;AACA,OARA;AASA,KA3BA;AA4BA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,IAAA,QArCA,oBAqCA,GArCA,EAqCA;AAAA;;AACA;AAEA,MAAA,oBAAA,CAAA;AAAA,QAAA,SAAA,EAAA,GAAA,CAAA;AAAA,OAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,EAAA,GAAA,MAAA,CAAA,WAAA,CAAA,oBAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,WAAA,CAAA,oBAAA,CAAA,EAAA;;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,YAAA,MAAA,CAAA,EAAA,GAAA,GAAA,GAAA,GAAA,CAAA,EAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,GAAA,EAAA,CAAA;AACA,OATA;AAUA,KAlDA;AAoDA;AACA,IAAA,WArDA,uBAqDA,CArDA,EAqDA,IArDA,EAqDA;AACA;AACA;AACA;AACA,UAAA,IAAA,GAAA,CAAA,CAAA,aAAA;AACA,UAAA,IAAA,GAAA,CAAA,CAAA,IAAA,CAAA,CAAA,MAAA,CAAA,gBAAA,CAAA;AACA,UAAA,IAAA,GAAA,CAAA,CAAA,IAAA,CAAA,CAAA,QAAA,CAAA,GAAA,CAAA;AACA,MAAA,IAAA,CAAA,QAAA,CAAA,cAAA;;AAEA,UAAA,IAAA,CAAA,QAAA,CAAA,WAAA,CAAA,EAAA;AACA,QAAA,IAAA,CAAA,WAAA,CAAA,WAAA;AACA,QAAA,MAAA,CAAA,UAAA,CAAA,YAAA;AACA,UAAA,IAAA,CACA,WADA,CACA,eADA,EAEA,WAFA,CAEA,cAFA,EAGA,QAHA,CAGA,SAHA;AAIA,SALA,EAKA,GALA;AAMA,OARA,MAQA;AACA,QAAA,IAAA,CAAA,QAAA,CAAA,WAAA;AACA,QAAA,MAAA,CAAA,UAAA,CAAA,YAAA;AACA,UAAA,IAAA,CACA,WADA,CACA,SADA,EAEA,WAFA,CAEA,cAFA,EAGA,QAHA,CAGA,eAHA;AAIA,SALA,EAKA,GALA;AAMA;AACA;AA/EA;AAjCA,CAAA","sourcesContent":["<template>\n  <div>\n    <section class=\"container\">\n      <div class=\"row active-with-click\">\n        <div class=\"col-md-3 col-sm-6 col-xs-12\" v-for=\"item in items\" :key=\"item.id\">\n          <article class=\"material-card Indigo\">\n            <h2>\n              <span>{{item.courseName}}</span>\n              <strong>\n                <i class=\"fa fa-fw fa-star\"></i>\n                {{item.teacherId}}\n              </strong>\n            </h2>\n            <div class=\"mc-content\">\n              <div class=\"img-container\">\n                <img class=\"img-responsive\" src=\"@/assets/cc.jpg\" />\n              </div>\n              <div class=\"mc-description\" style=\"height: 190px\">{{item.introduction}}</div>\n            </div>\n            <a class=\"mc-btn-action\" @click=\"changeClass($event,item)\">\n              <i class=\"fa fa-bars\"></i>\n            </a>\n            <div class=\"mc-footer\">\n              <!-- <a class=\"fa fa-fw\" style=\"width: 185px\" v-show=\"!result\" @click=\"changestatus\">加入课程</a> -->\n              <a class=\"fa fa-fw\" style=\"width: 185px\"  @click=\"goCourse(item)\">查看详情</a>\n            </div>\n          </article>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport { queryMyList } from \"@/api/modules/student\";\nimport { coursebyinterest, getpcategorybycourse } from \"@/api/modules/course\";\nimport { getCookie, setCookie, removeCookie } from \"@/libs/cookie\";\nimport $ from \"jquery\";\nexport default {\n  name: \"Cardies\",\n  data() {\n    return {\n      items: {\n        result: \"\"\n      },\n      form: {\n        studentId: \"\",\n        courseId: \"\"\n      },\n      subcategory: \"\",\n      id: \"\",\n      result: false,\n      studentid: \"\",\n      myuser: \"\",\n      list :'',\n    };\n  },\n  created() {\n    //    self = this\n    // window.change= self.change\n  },\n\n  mounted() {\n    this.myuser = JSON.parse(getCookie(\"user\") || \"{}\");\n    this.studentid = this.myuser.etc.id;\n\n    this.form.studentId = this.myuser.etc.id;\n\n    this.getcbyi();\n  },\n\n  methods: {\n    // //查询课程是否在选课表中\n    // queryMyList1(item) {\n    //   this.form.courseId = item.id;\n    //   queryMyList(this.form)\n    //     .then(resp => {  \n    //       item.result = resp.data.data\n    //       this.result = resp.data.data\n    //       console.log(  this.result);\n    //     })\n    //     .catch(data => {\n    //       this.toast(data, 2);\n    //     });\n    // },\n\n    getcbyi() {\n      //根据兴趣推荐课程\n\n      coursebyinterest({ student_id: this.studentid })\n        .then(resp => {\n          this.items = resp.data.data;\n          console.log(this.items);\n          //  this.queryMyList1()\n        })\n        .catch(data => {\n          this.toast(data, 2);\n        });\n    },\n    // //改变课程状态\n    // changestatus() {\n    //   this.$message({\n    //     message: \"加入课程成功\",\n    //     type: \"success\"\n    //   });\n    //   this.result = !this.result;\n    // },\n    //跳转兴趣课程\n    goCourse(row) {\n      //根据课程Id获取子类别\n\n      getpcategorybycourse({ course_id: row.id })\n        .then(res => {\n          this.subcategory = res.data.data;\n          this.id = this.subcategory.parent_category_info.id;\n          console.log(this.subcategory.parent_category_info.id);\n          this.navigator(\"/index/\" + this.id + \"/\" + row.id);\n        })\n        .catch(err => {\n          this.toast(err, 2);\n        });\n    },\n\n    // 点击卡片上的按钮，动态更改卡片样式\n    changeClass(e, item) {\n       //查询课程状态\n    // this.queryMyList1(item);\n      //动态更改卡片样式\n      var that = e.currentTarget;\n      var card = $(that).parent(\".material-card\");\n      var icon = $(that).children(\"i\");\n      icon.addClass(\"fa-spin-fast\");\n\n      if (card.hasClass(\"mc-active\")) {\n        card.removeClass(\"mc-active\");\n        window.setTimeout(function() {\n          icon\n            .removeClass(\"fa-arrow-left\")\n            .removeClass(\"fa-spin-fast\")\n            .addClass(\"fa-bars\");\n        }, 800);\n      } else {\n        card.addClass(\"mc-active\");\n        window.setTimeout(function() {\n          icon\n            .removeClass(\"fa-bars\")\n            .removeClass(\"fa-spin-fast\")\n            .addClass(\"fa-arrow-left\");\n        }, 800);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n@import \"../dist/material-cards.css\";\n@import \"http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css\";\n@import \"http://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css\";\n@import \"http://fonts.googleapis.com/css?family=Raleway:400,300,200,500,600,700\";\n</style>\n"],"sourceRoot":"src/components"}]}