A;;AACA,MAAA,UAAA,CAAA,YAAA;AACA,YAAA,OAAA,CAAA,IAAA,CAAA,KAAA,CAAA,EAAA;AACA,UAAA,QAAA;AACA,SAFA,MAEA;AACA,UAAA,QAAA,CAAA,IAAA,KAAA,CAAA,YAAA,CAAA,CAAA;AACA;AACA,OANA,EAMA,GANA,CAAA;AAOA,KAfA;;AAiBA,WAAA;AACA,MAAA,UAAA,EAAA;AACA,QAAA,QAAA,EAAA,EADA;AAEA,QAAA,QAAA,EAAA,EAFA;AAGA,QAAA,KAAA,EAAA,EAHA;AAIA,QAAA,GAAA,EAAA,KAAA,MAAA,CAAA,KAAA,CAAA,GAJA,CAIA;;AAJA,OADA;AAOA,MAAA,aAAA,EAAA,EAPA;AAQA,MAAA,WAAA,EAAA;AACA,QAAA,QAAA,EAAA;AAAA,UAAA,SAAA,EAAA,WAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA;AAEA,QAAA,QAAA,EAAA;AAAA,UAAA,SAAA,EAAA,aAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAFA;AAGA,QAAA,KAAA,EAAA;AAAA,UAAA,SAAA,EAAA,SAAA;AAAA,UAAA,OAAA,EAAA;AAAA;AAHA;AARA,KAAA;AAcA,GA3DA;AA4DA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AAAA;;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA;AACA;AACA,UAAA,UAAA,CAAA;AACA,YAAA,QAAA,EAAA,KAAA,CAAA,UAAA,CAAA,QADA;AAEA,YAAA,QAAA,EAAA,KAAA,CAAA,UAAA,CAAA,QAFA;AAGA,YAAA,KAAA,EAAA,KAAA,CAAA,UAAA,CAAA,KAHA;AAIA,YAAA,GAAA,EAAA,KAAA,CAAA,UAAA,CAAA,GAJA;AAKA,YAAA,SAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA,aAAA;AALA,WAAA,CAAA,CAOA,IAPA,CAOA,UAAA,IAAA,EAAA;AACA,gBAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,SAAA,CAAA,MAAA,CAAA,IAAA,IAAA,CAAA;AACA,YAAA,IAAA,CAAA,QAAA,GAAA,KAAA,CAAA,UAAA,CAAA,QAAA;AACA,YAAA,IAAA,CAAA,KAAA,GAAA,KAAA,CAAA,UAAA,CAAA,KAAA;AACA,YAAA,SAAA,CAAA,MAAA,EAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA,CAAA;;AACA,YAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,OAAA,EAAA,CAAA;;AACA,YAAA,KAAA,CAAA,SAAA,CAAA,OAAA;AACA,WAdA,EAeA,KAfA,CAeA,UAAA,IAAA,EAAA;AACA,YAAA,KAAA,CAAA,KAAA,CAAA,IAAA,EAAA,CAAA;AACA,WAjBA;AAkBA;AACA,OAvBA;AAwBA;AA1BA;AA5DA,CAAA","sourcesContent":["<template>\n  <div class=\"infoRefine-container\">\n    <div class=\"imgContainer\">\n      <img src=\"../assets/infoRefineImg.jpg\" alt />\n    </div>\n    <div class=\"inputForm\">\n      <h2>个人信息完善</h2>\n      <el-form ref=\"refineForm\" label-width=\"80px\" :model=\"refineForm\" :rules=\"refineRules\">\n        <el-form-item label=\"用户名\" prop=\"username\">\n          <el-input v-model=\"refineForm.username\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"密码\" prop=\"password\">\n          <el-input v-model=\"refineForm.password\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"邮箱\" prop=\"email\">\n          <el-input\n            ref=\"email\"\n            v-model=\"refineForm.email\"\n            name=\"email\"\n            type=\"text\"\n            autocomplete=\"on\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"手机号\" prop=\"tel\">\n          <el-input v-model=\"refineForm.tel\" disabled></el-input>\n        </el-form-item>\n        <el-form-item label=\"兴趣选择:\" prop=\"tel\">\n          <el-checkbox-group v-model=\"checkboxGroup\" size=\"big\">\n            <el-checkbox label=\"商业\" border></el-checkbox>\n            <el-checkbox label=\"艺术与人文\" border></el-checkbox>\n            <el-checkbox label=\"农业\" border></el-checkbox>\n            <el-checkbox label=\"物理学\" border></el-checkbox>\n            <el-checkbox label=\"数学与逻辑\" border></el-checkbox>\n            <el-checkbox label=\"计算机科学\" border></el-checkbox>\n          </el-checkbox-group>\n        </el-form-item>\n      </el-form>\n      <!-- <div>{{checkboxGroup}}</div> -->\n      <el-button class=\"btn\" type=\"primary\" round @click=\"updateInfo\">进入平台</el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { getCookie, setCookie, removeCookie } from \"@/libs/cookie\";\nimport Vcode from \"vue-puzzle-vcode\";\nimport { refineInfo } from \"@/api/modules/user\";\n\nexport default {\n  name: \"infoRefine\",\n  data() {\n    //用户名验证\n    var checkLength = (rule, value, callback) => {\n      if (!value) {\n        return callback(new Error(\"用户名不能为空\"));\n      }\n\n      if (value.length >= 5) {\n        callback();\n      } else {\n        callback(new Error(\"长度不能小于5\"));\n      }\n    };\n    //密码验证\n    var checkPassword = (rule, value, callback) => {\n      if (!value) {\n        return callback(new Error(\"密码不能为空\"));\n      }\n\n      if (value.length >= 5) {\n        callback();\n      } else {\n        callback(new Error(\"长度不能小于6位\"));\n      }\n    };\n    //邮箱验证\n    var userEmail = (rule, value, callback) => {\n      const mailReg = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(.[a-zA-Z0-9_-])+/;\n      // if (!value) {\n      //   return callback();\n      // }\n      if (!value) {\n        return callback(new Error(\"邮箱不能为空\"));\n      }\n      setTimeout(() => {\n        if (mailReg.test(value)) {\n          callback();\n        } else {\n          callback(new Error(\"请输入正确的邮箱格式\"));\n        }\n      }, 100);\n    };\n\n    return {\n      refineForm: {\n        username: \"\",\n        password: \"\",\n        email: \"\",\n        tel: this.$route.query.tel //接收上一个界面传来的参数\n      },\n      checkboxGroup: [],\n      refineRules: {\n        username: { validator: checkLength, trigger: \"blur\" },\n        password: { validator: checkPassword, trigger: \"blur\" },\n        email: { validator: userEmail, trigger: \"blur\" }\n      }\n    };\n  },\n  methods: {\n    updateInfo() {\n      this.$refs.refineForm.validate(valid => {\n        if (valid) {\n              // console.log(JSON.stringify(this.checkboxGroup));\n              // console.log(typeof(JSON.stringify(this.checkboxGroup)));\n          refineInfo({\n            username: this.refineForm.username,\n            password: this.refineForm.password,\n            email: this.refineForm.email,\n            tel: this.refineForm.tel,\n            interests: JSON.stringify(this.checkboxGroup)\n          })\n            .then(resp => {\n              let user = JSON.parse(getCookie(\"user\") || \"{}\");\n              user.username = this.refineForm.username;\n              user.email = this.refineForm.email;\n              setCookie(\"user\", JSON.stringify(user));\n              this.toast(resp.data.message, 1);\n              this.navigator(\"/Home\");\n            })\n            .catch(data => {\n              this.toast(data, 2);\n            });\n        }\n      });\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" >\nhtml {\n  // background-image: url(\"../assets/loginBack.jpeg\");\n  background-repeat: no-repeat;\n  background-size: auto;\n  // background-color: blue;\n}\nh2 {\n  color: #2897ff;\n}\n\n.infoRefine-container {\n  margin: 50px auto;\n  height: 650px;\n  width: 1000px;\n  border: solid 1px none;\n  border-radius: 20px;\n  box-shadow: #d5dadf 6px 9px 33px 5px;\n  background-color: rgba($color: white, $alpha: 0.7);\n}\n\n.imgContainer {\n  float: left;\n  height: 650px;\n  width: 400px;\n  background-color: rgba($color: white, $alpha: 0.1);\n}\n\n.imgContainer img {\n  border: solid 1px none;\n  border-top-left-radius: 20px;\n  border-bottom-left-radius: 20px;\n  height: 100%;\n  width: 100%;\n}\n\n.inputForm {\n  float: left;\n  height: 650px;\n  width: 470px;\n  padding: 0px 65px;\n}\n\n.el-checkbox.is-bordered {\n  margin: 10px;\n}\n\n.btn {\n  margin: auto 220px;\n}\n</style>\n\n\n"],"sourceRoot":"src/views"}]}